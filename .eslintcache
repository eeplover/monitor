thub/monitor/src/monitor-error.ts",["38","39"],[],"import { merge } from './utils';\nimport Monitor from './monitor';\nclass ErrorMonitor extends Monitor {\n  public data: SendData;\n  public send: SendFn;\n  constructor(options: Options) {\n    super(options);\n    const { data, send } = options;\n    this.data = merge({}, data);\n    this.send = send;\n  }\n  public init(data?: SendData): void {\n    this.data = merge({}, this.data, data);\n    // 捕获全局 Promise 异常\n    window.addEventListener('unhandledrejection', (event: PromiseRejectionEvent) => {\n      this.report({ message: event.reason });\n    });\n    // 捕获全局 js 异常\n    window.addEventListener('error', (event: ErrorEvent) => {\n      this.report(event);\n    });\n    // 捕获文件加载异常\n    window.addEventListener(\n      'error',\n      (event: ErrorEvent) => {\n        this.report(event);\n      },\n      true\n    );\n    // 捕获请求异常，封装 XMLHttpRequest 和 Fetch\n    const rawXMLHttpRequest = window.XMLHttpRequest;\n    const rawFetch = window.fetch;\n    // TODO\n  }\n\n  public report(info: SendData): void {\n    info = merge({}, this.data, info);\n    this.send(info);\n  }\n}\n\nexport default ErrorMonitor;\n","/Users/eplover/Code/github/monitor/src/monitor-log.ts",[],[],"/Users/eplover/Code/github/monitor/src/monitor-performance.ts",[],[],"/Users/eplover/Code/github/monitor/src/monitor.ts",[],[],"/Users/eplover/Code/github/monitor/src/utils.ts",["40"],[],"export function merge(target, ...sources) {\n  return target;\n}\n",{"ruleId":"41","severity":1,"message":"42","line":1,"column":8,"nodeType":"43","messageId":"44","endLine":1,"endColumn":11},{"ruleId":"41","severity":1,"message":"45","line":2,"column":8,"nodeType":"43","messageId":"44","endLine":2,"endColumn":20},{"ruleId":"41","severity":1,"message":"46","line":3,"column":8,"nodeType":"43","messageId":"44","endLine":3,"endColumn":26},{"ruleId":"41","severity":1,"message":"47","line":31,"column":11,"nodeType":"43","messageId":"44","endLine":31,"endColumn":28},